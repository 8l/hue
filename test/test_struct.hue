bar = struct            # constant <{ i64, %type.S1d* }>
  a = 1
  b = struct            # constant <{ double }>
    c = 2.3

foo = func (n Int)
  struct                # allocates <{ %type.S1x* }>
    x = struct          # allocates <{ i64 }>
      y = n

baz = func (n Int)
  y = n * 2
  struct bob = y        # allocates <{ i64 }>

st1 = baz 20

bar:a    # 1
bar:b:c  # 2.3
st1:bob  # 40

